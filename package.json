{
  "name": "invitations",
  "version": "1.2.0",
  "api": "invitations/v1",
  "description": "invitations micro service",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node build/src/index.js",
    "build": "tsc",
    "buildw": "tsc -w",
    "debug": "API_SECRET=42 nodemon --exec \" ts-node src/index.ts\" -w src -e ts | bunyan -o short --color",
    "test": "API_SECRET=42 mocha --color --require ts-node/register --extensions ts tests/**/test-*.ts | bunyan -l error -o short --color"
  },
  "author": "Jean-Christophe Hoelt <hoelt@fovea.cc>",
  "license": "Private",
  "dependencies": {
    "authdb": "^0.3.0",
    "blanket": "^1.1.10",
    "bunyan": "^1.8.12",
    "concat-stream": "^1.4.10",
    "eslint": "^4.3.0",
    "fakeredis": "^0.3.4",
    "forever": "^3.0.0",
    "mocha": "^7.2.0",
    "node-statsd": "^0.1.1",
    "redis": "^3.0.2",
    "restify": "^8.5.1",
    "restify-errors": "^8.0.2",
    "source-map-support": "^0.5.19",
    "superagent": "^5.2.2",
    "ts-node": "^8.10.1",
    "vasync": "^1.6.4"
  },
  "config": {
    "blanket": {
      "pattern": [
        "src"
      ],
      "data-cover-never": "node_modules",
      "loader": "./node-loaders/coffee-script"
    }
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.5",
    "@types/redis": "^2.8.21",
    "@types/restify": "^8.4.2",
    "@types/restify-errors": "^4.3.3",
    "expect.js": "^0.3.1",
    "lodash": "^4.17.4",
    "nodemon": "^2.0.4",
    "testdouble": "^3.2.6",
    "typescript": "^3.9.3"
  }
}
